    <script>
        class Cellphone
        {
        constructor(brand, model, color, year, capacity)
        {
        this.cellphonebrand = brand;
        this.cellphonemodel = model;
        this.cellphonecolor = color;
        this.cellphoneyear = year;
        this.cellphonecapacity = capacity;
        }
        cellphone1(){
        return "This " + this.cellphonebrand + " " + this.cellphonemodel + " is a color new " + this.cellphoneyear + " " + this.cellphonecapacity;
        }
        }


        mycellphone = newCellphone("Apple", "iPhone11", "purple", "2019", "256GB");
        document.getElementById("demo").innerHTML = mycellphone.cellphone1();

